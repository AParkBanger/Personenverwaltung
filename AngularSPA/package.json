  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:api": "@openapi-generator-cli generate -i ./openapi.json -g typescript-angular -o src/app/core/api/v1 -p=removeOperationIdPrefix=true",
